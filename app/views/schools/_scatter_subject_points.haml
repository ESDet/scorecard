.graph-point.school(data-proficiency="#{school_proficiency}" data-growth="#{school_growth}")
  .point-popup.hide
    .row
      .name= school_name
    .row
      .subject= subject.upcase
    .row
      .left
        .text.big
        .text.small
          COLLEGE READY
      .right
        .state
          #{state_proficiency}%
          %span STATE
        .detroit
          #{detroit_proficiency}%
          %span DETROIT
    .row
      .act-growth
        .left
        .right
.graph-point.detroit(data-proficiency="#{detroit_proficiency}" data-growth="#{detroit_growth}")
  .point-popup.hide
    .row
      .name Detroit
    .row
      .subject= subject.upcase
    .row
      .college-ready
        .text.big
        .text.small
          COLLEGE READY
    .row
      .act-growth
        .left
        .right
