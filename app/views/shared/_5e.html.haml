- unless element[2].nil?
  .grid_3.alpha{:style => 'min-height: 50px; margin: 3px 0;'}
    %h4= element[0]
    %div= element[1]
  .grid_3.omega
    .elements_score.noprint{:style => 'position: relative; width: 220px;'}
      .bar{:style => "right: #{125-(1.25*element[2]).to_i}px;"}
      .circled.ib{:style => "position: absolute; right: #{125-(1.25*element[2]).to_i}px;"}= element[2].to_i
    .printonly.centered{:style => 'margin-bottom: 1em; padding-top: 0.5em;'}
      %h4 #{element[2].to_i}%
      %br/
      
  .clear_floats
  